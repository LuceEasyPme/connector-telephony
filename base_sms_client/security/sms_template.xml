<odoo>

    <record model="ir.model.access" id="sms_template_access_all">
        <field name="name">sms.template access all</field>
        <field name="model_id" ref="model_sms_template"/>
        <field name="group_id" ref="base.group_user"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_create" eval="0"/>
        <field name="perm_write" eval="0"/>
        <field name="perm_unlink" eval="0"/>
    </record>

    <record model="ir.model.access" id="sms_template_access_user">
        <field name="name">sms.template access sms user</field>
        <field name="model_id" ref="model_sms_template"/>
        <field name="group_id" ref="base_sms_client.group_sms_user"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>

</odoo>
